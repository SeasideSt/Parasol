initialize-release
initializeWithName: initialName value: initialValue domain: initialDomain path: initialPath expiry: initialExpiry isSecure: initialIsSecure isHTTPOnly: initialIsHTTPOnly

	self initialize.
	name := initialName.
	value := initialValue.
	domain := initialDomain.
	path := initialPath.
	expiry := initialExpiry.
	isSecure := initialIsSecure.
	isHTTPOnly := initialIsHTTPOnly.
